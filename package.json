{
	"name": "vscode-resx-editor-v2",
	"displayName": "ResX Editor v2",
	"description": "Reworked ResX Editor based on DominicVonk's ResX Editor",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.49.0"
	},
	"publisher": "levimayer21",
	"categories": [
		"Other"
	],
	"keywords": [
		"resx",
		"resw"
	],
	"activationEvents": [
		"onCustomEditor:resx.editor"
	],
	"bugs": {
		"url": "https://github.com/levimayer21/vscode-resx-v2/issues"
	},
	"license": "MIT",
	"galleryBanner": {
		"color": "#fff",
		"theme": "light"
	},
	"icon": "resx.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/levimayer21/vscode-resx.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "resx.editor",
				"displayName": "ResX",
				"selector": [
					{
						"filenamePattern": "*.resx"
					},
					{
						"filenamePattern": "*.resw"
					}
				],
				"priority": "default"
			}
		],
		"languages": [
			{
				"id": "xml",
				"extensions": [
					".resx",
					".resw"
				]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@vscode/codicons": "^0.0.32",
		"resx": "^2.0.3"
	}
}
